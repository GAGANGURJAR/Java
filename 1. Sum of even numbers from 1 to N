import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
      Scanner ac = new Scanner (System.in);
      int n = ac.nextInt();
      int i=2;
      int sum =0;
      while(i<=n){
          sum = sum + i ; 
          i = i+2;
          }
          System.out.println("Sum is " + sum);
  }
}
