import java.util.Scanner;

public class loop {
    public static void main(String[] args) {

       Scanner ac = new Scanner(System.in);
        System.out.println("Enter number ");
        int n =  ac.nextInt();
       for (int i =1;i<=n;i++){
           for (int j = 1; j<=n;j++){
               System.out.print("*");
           }

           System.out.println();

       }
    }
}
