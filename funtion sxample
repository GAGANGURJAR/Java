public class Functions {
    public static int gagan(String item1 , String item2, int money){
int item1cost = 10;
int item2cost = 20;
        System.out.println("Bringing "+item1+" and "+item2 + " "+money);
        return money - item1cost - item2cost;

    }
    public static void main(String[] args) {
        int money = 100;

       int remmoney =  gagan("Apple","banana",money);
        System.out.println(remmoney);
    }
}
