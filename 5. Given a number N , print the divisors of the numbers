import java.util.Scanner;

public class loop {
    public static void main(String[] args) {
       Scanner ac = new Scanner(System.in);
        System.out.println("Enter value of n ");
        int n  =  ac.nextInt();
        for (int i = 1 ;i<=n;i++)
        {
            if (n%i==0) {
                System.out.println("Divisor"+"    "+i);


            }
            else {
                System.out.println("Not Divisor"+" "+i );
            }

        }
    }
}
